            }
          ],
          "Annotations": []
        },
        {
          "FriendlyName": "Grid Spacing",
          "InternalName": "steamaudio_reverb_grid_spacing",
          "VariableType": "Float",
          "Description": "The horizontal distance (in meters) between adjacent probes.",
          "Options": [],
          "Annotations": []
        },
        {
          "FriendlyName": "Height Above Floor",
          "InternalName": "steamaudio_reverb_height_above_floor",
          "VariableType": "Float",
          "Description": "The height (in meters) above the floor at which probes should be placed..",
          "Options": [],
          "Annotations": []
        },
        {
          "FriendlyName": "Baking Rays",
          "InternalName": "steamaudio_reverb_rays",
          "VariableType": "Integer",
          "Description": "The number of rays traced from the listener when baking reflections or reverb. Increasing this value results in more accurate reflections, at the cost of increased bake times.",
          "Options": [],
          "Annotations": []
        },
        {
          "FriendlyName": "Baking Bounces",
          "InternalName": "steamaudio_reverb_bounces",
          "VariableType": "Integer",
          "Description": "The number of times each ray traced from the listener is reflected when baking reflections or reverb. Increasing this value results in longer, more accurate reverb tails, at the cost of increased bake times.",
          "Options": [],
          "Annotations": []
        },
        {
          "FriendlyName": "Baking Duration",
          "InternalName": "steamaudio_reverb_ir_duration",
          "VariableType": "Float",
          "Description": "The duration (in seconds) of the IRs generated when baking reflections or reverb. Increasing this value results in longer, more accurate reverb tails, at the cost of increased disk space usage.",
          "Options": [],
          "Annotations": []
        },
        {
          "FriendlyName": "Baking Ambisonic Order",
          "InternalName": "steamaudio_reverb_ambisonic_order",
          "VariableType": "Integer",
          "Description": "The Ambisonic order of the IRs generated when baking reflections or reverb. Increasing this value results in more accurate directional variation of reflected sound, at the cost of increased disk space usage.",
          "Options": [],
          "Annotations": []
        },
        {
          "FriendlyName": "Rebake Option",
          "InternalName": "steamaudio_pathing_rebake_option",
          "VariableType": "Choices",
          "Description": "\u003Cb\u003ECleanup\u003C/b\u003E: Pathing bake is disabled. Baked data will be cleaned up during map compile in Hammer or mapbuilder.\u003Cbr /\u003E\u003Cb\u003EManual\u003C/b\u003E: Pathing needs to be baked manually. Baked data is added or updated during map compile in Hammer but untouched by mapbuilder.\u003Cbr /\u003E\u003Cb\u003EAuto\u003C/b\u003E: Pathing is baked automatically during map compile. Baked data is added or updated during map compile in Hammer or mapbuilder.",
          "Options": [
            {
              "Name": "Cleanup",
